{
  "name": "no-stack-cli",
  "description": "tools for stack generation from templates and an api",
  "version": "0.0.2",
  "author": "YizYah @YizYah",
  "bin": {
    "nostack": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/YizYah/no-stack-cli/issues"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/dotenv": "^6.1.1",
    "byline": "^5.0.0",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.1",
    "dotenv": "^8.0.0",
    "error-ex": "^1.3.2",
    "fs-extra": "^8.1.0",
    "graphql-request": "^1.8.2",
    "inflection": "^1.12.0",
    "map-obj": "^4.1.0",
    "np": "^5.0.3",
    "object.map": "^1.0.1",
    "path": "^0.12.7",
    "pify": "^4.0.1",
    "pluralize": "^8.0.0",
    "replace-in-file": "^4.1.1",
    "shelljs": "^0.8.3",
    "tslib": "^1",
    "untildify": "^4.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/YizYah/no-stack-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nostack",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YizYah/no-stack-cli.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "updateReadme": "oclif-dev readme",
    "test": "echo \"No test specified\"",
    "oldtest": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "createNoStack": "bin/create-no-stack-app",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "directories": {
    "test": "test"
  }
}
